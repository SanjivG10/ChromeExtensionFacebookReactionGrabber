{
    "name": "Get Reaction",
    "version": "1.0",
    "description": "Get Haha reaction only! ",
    "manifest_version": 2, 
    "content_scripts":[
        {   
            "matches": ["https://*.facebook.com/*"],
            "js":["jquery.js","content.js"]
        }
    ], 
    "background":{
        "scripts": ["background.js"]
    }, 
    "browser_action": {
        "default_icon": "fb.png", 
        "default_popup": "popup.html"
    },  
    "permissions": [
        "storage",
        "unlimitedStorage"
    ]
}

